{
  "name": "toolkit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "cross-env NODE_ENV=development npx tsc && cross-env NODE_ENV=development node dist/main.js"
  },
  "keywords": [
    "toolkits",
    "utils"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@/modules": "dist/modules",
    "@/common": "dist/common",
    "@/config": "dist/config",
    "@/constants": "dist/constants",
    "@/interfaces": "dist/interfaces",
    "@/utils": "dist/utils",
    "@/global": "dist/global",
    "@/templates": "dist/templates"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "dependencies": {
    "mongodb": "^5.7.0",
    "cross-env": "^7.0.3",
    "joi": "^17.9.2",
    "module-alias": "^2.2.3"
  }
}
